<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>

	<package name="admin" namespace="/admin" extends="struts-default">

		<interceptors>
			<interceptor name="authenticationInterceptor"
				class="web.security.AuthenticationInterceptor" />
 
			<interceptor name="adminRoleInterceptor"
				class="web.security.AdminRoleInterceptor" />

			<interceptor-stack name="securedStack">
				<interceptor-ref name="authenticationInterceptor" />
				<interceptor-ref name="adminRoleInterceptor" />
				<interceptor-ref name="defaultStack" />
			</interceptor-stack>
		</interceptors>

		<default-interceptor-ref name="securedStack" />

		<global-results>
			<result name="login" type="redirect">
				/common/login.jsp
			</result>
		</global-results>

		<action name="hw">
			<result>/admin/hw.jsp</result>
		</action>

		<action name="viewhw" class="web.admin.ViewHealthWorkers">
			<result>/admin/hw-view.jsp</result>
		</action>

		<action name="viewhwform">
			<result>/admin/hw-add.jsp</result>
		</action>

		<action name="addhw" class="web.admin.AddHealthWorker">
			<result name="input">/admin/result-addition.jsp</result>
			<result name="success">/admin/result-addition.jsp</result>
		</action>

		<action name="deletehw" class="web.admin.DeleteHealthWorker">
			<result name="success" type="redirect">
				/admin/hw.action
			</result>
		</action>

		<action name="department">
			<result>/admin/department.jsp</result>
		</action>

		<action name="viewdepartment"
			class="web.admin.ViewDepartments">
			<result>/admin/department-view.jsp</result>
		</action>


		<action name="viewdepartmentform">
			<result>/admin/department-add.jsp</result>
		</action>

		<action name="adddepartment" class="web.admin.AddDepartment">
			<result name="input">/admin/result-addition.jsp</result>
			<result name="success">/admin/result-addition.jsp</result>
		</action>

		<action name="patient">
			<result>/admin/patient.jsp</result>
		</action>

		<action name="viewpatient" class="web.admin.ViewPatients">
			<result>/admin/patient-view.jsp</result>
		</action>

		<action name="viewpatientform">
			<result>/admin/patient-add.jsp</result>
		</action>

		<action name="addpatient" class="web.admin.AddPatient">
			<result name="input">/admin/result-addition.jsp</result>
			<result name="success">/admin/result-addition.jsp</result>
		</action>

		<action name="updatedepartmentmodules"
			class="web.admin.UpdateDepartmentModules">
			<result name="input">/admin/result-addition.jsp</result>
		</action>


		<action name="updatedepartmentanalyses"
			class="web.admin.UpdateDepartmentAnalyses">
			<result name="input">/admin/result-addition.jsp</result>
		</action>


		<action name="updatehwdata"
			class="web.admin.UpdateDataHealthWorker">
			<result name="input">/admin/result-addition.jsp</result>
			<result>/admin/hw-update.jsp</result>
		</action>


		<action name="updatehw" class="web.admin.UpdateHealthWorker">
			<result name="input">/admin/result-addition.jsp</result>
			<result name="success">/admin/result-addition.jsp</result>
		</action>

		<action name="module">
			<result>/admin/module.jsp</result>
		</action>

		<action name="viewmodule" class="web.admin.ViewModules">
			<result>/admin/module-view.jsp</result>
		</action>


		<action name="viewmoduleform">
			<result>/admin/module-add.jsp</result>
		</action>

		<action name="addmodule" class="web.admin.AddModule">
			<result name="input">/admin/result-addition.jsp</result>
			<result name="success">/admin/result-addition.jsp</result>
		</action>

		<action name="card">
			<result>/admin/card.jsp</result>
		</action>

		<action name="viewcard" class="web.admin.ViewCards">
			<result>/admin/card-view.jsp</result>
		</action>

		<action name="viewcardform">
			<result>/admin/card-add.jsp</result>
		</action>

		<action name="cardexplorer" class="web.admin.CardExplorer">
			<result name="success">/admin/cardexplorer.jsp</result>
			<result name="none" type="redirect">
				/admin/card.action
			</result>
		</action>

		<action name="addcard" class="web.admin.AddCard">
			<result name="input">/admin/result-addition.jsp</result>
			<result name="success">
				/admin/result-addition-success-card.jsp
			</result>
		</action>
		
		<action name="viewcarddata">
			<result>/admin/card-view-data.jsp</result>
		</action>

		<action name="viewmoduledataform">
			<result>/admin/card-module-data-form.jsp</result>
		</action>
		
		<action name="loadmoduledataform" class="web.admin.LoadModuleDataForm">
			<result name="input">/admin/result-addition.jsp</result>
			<result name="success">/admin/card-form-moduledata.jsp</result>
		</action>
		
		<action name="loadanalysisdataform" class="web.admin.LoadAnalysisDataForm">
			<result name="input">/admin/result-addition.jsp</result>
			<result name="success">/admin/card-form-analysisdata.jsp</result>
		</action>
		
		<action name="savemoduledata" class="web.admin.SaveModuleData">
			<result name="input">/admin/result-addition.jsp</result>
			<result name="success">/admin/result-addition.jsp</result>
		</action>
		
		<action name="saveanalysisdata" class="web.admin.SaveAnalysisData">
			<result name="input">/admin/result-addition.jsp</result>
			<result name="success">/admin/result-addition.jsp</result>
		</action>
		
		<action name="analysis">
			<result>/admin/analysis.jsp</result>
		</action>

		<action name="viewanalysis" class="web.admin.ViewAnalyses">
			<result>/admin/analysis-view.jsp</result>
		</action>


		<action name="viewanalysisform">
			<result>/admin/analysis-add.jsp</result>
		</action>

		 <action name="addanalysis" class="web.admin.AddAnalysis">
			<result name="input">/admin/result-addition.jsp</result>
			<result name="success">/admin/result-addition.jsp</result>
		</action>
		
		<action name="deletemodule" class="web.admin.DeleteModule">
			<result name="success" type="redirect">
				/admin/module.action
			</result>
		</action>
	</package>

</struts>