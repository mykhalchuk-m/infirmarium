	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:s="urn:import:com.infirmarium.client.components.elements.simple">
	<g:VerticalPanel addStyleNames="full-width">
		<g:HorizontalPanel ui:field="topSlot" addStyleNames="page-top-slot">
			<s:LargeIconWidget ui:field="icon" />
		</g:HorizontalPanel>
		<g:SimplePanel ui:field="contentSlot"
			addStyleNames="page-separator page-content-slot">
