<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" 
	xmlns:ic="urn:import:com.infirmarium.client.core.components.elements.core">
	<g:HTMLPanel addStyleNames="main">
		<div class="header">
			<div class="inf-logo-large" />
			<ic:NavigationWidget ui:field="navigationWidget"
				addStyleNames="inf-navigation-panel float-right" />
		</div>
		<ic:ScreenManagerWidget ui:field="screenPanel"
			addStyleNames="content" />
	</g:HTMLPanel>
</ui:UiBinder>